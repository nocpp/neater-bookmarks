{
	"name": "Neater_Bookmarks_V3",
	"version": "1.0.0",
	"manifest_version": 3,
	"description": "Neater_Bookmarks_V3",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"action": {
		"default_icon": {
      "128": "icon128.png"
    },
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"permissions": [
		"bookmarks",
		"tabs"
	],
  "host_permissions": [
		"<all_urls>"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
	"minimum_chrome_version": "9",
	"default_locale": "en",
	"omnibox": {
		"keyword" : "*"
	}
}